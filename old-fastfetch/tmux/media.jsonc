// Use like `fastfetch -c ~/.config/fastfetch/tmux/media.jsonc`
{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"logo": "none",
	"display": {
		"pipe": true,
		"key": {
			"type": "none"
		}
	},
	"modules": [
		{
			"type": "player",
			"format": "{player}"
		},
		{
			"type": "media",
			"format": "{status}\n{artist} - {combined}"
		},
	]
}
